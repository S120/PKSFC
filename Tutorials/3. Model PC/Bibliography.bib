	Journal = {Journal of Political Economy},
	Keywords = {Equilibrium; Expectation; Continuous vs period; stock and flows},
	Methodology = {Theoretical; Analytical},
	Number = {2},
	Title = {{On Two Specifications of Asset Equilibrium in Macroeconomic Models}},
	Volume = {83},
	Year = {1975},
