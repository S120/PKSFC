	Date-Modified = {2012-10-30 12:16:14 +0100},
	Editor = {Louis-Philippe Rochon},
	Keywords = {state of confidence, lender risk, financial crisis, real-financial interaction},
	Methodology = {Theoretical/Simulation},
	Publisher = {Edward Elgar},
	Sectors = {government, firms, households, Banks and NBFI, central bank},
	Title = {{Financial Crisis, State of Confidence and Economic Policies in a Post-Keynesian Stock-Flow Consistent Model}},
